<?xml version="1.0" encoding="UTF-8"?>
<model name="fill-pdf:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <description>Fill pdf plugin model</description>
    <author>Aimprosoft</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco System Model Definitions -->
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
    </imports>

    <namespaces>
        <namespace uri="http://www.aimprosoft.com/model/content/1.0" prefix="fill-pdf"/>
    </namespaces>

    <types>
        <type name="fill-pdf:document">
            <title>Fill PDF form document type</title>
            <parent>cm:content</parent>
            <mandatory-aspects>
                <aspect>fill-pdf:isFillPdf</aspect>
            </mandatory-aspects>
        </type>
    </types>

    <aspects>
        <aspect name="fill-pdf:isFillPdf">
            <title>Is fill PDF</title>
            <description>Special aspect for checking document type</description>
        </aspect>
    </aspects>

</model>